DROP TABLE MANITOU_TEST.APEX$_WS_HISTORY CASCADE CONSTRAINTS;

CREATE TABLE MANITOU_TEST.APEX$_WS_HISTORY
(
  ROW_ID               NUMBER                   NOT NULL,
  WS_APP_ID            NUMBER                   NOT NULL,
  DATA_GRID_ID         NUMBER                   NOT NULL,
  COLUMN_NAME          VARCHAR2(255 BYTE),
  OLD_VALUE            VARCHAR2(4000 BYTE),
  NEW_VALUE            VARCHAR2(4000 BYTE),
  APPLICATION_USER_ID  VARCHAR2(255 BYTE),
  CHANGE_DATE          DATE
)
TABLESPACE USERS
PCTUSED    0
PCTFREE    10
INITRANS   1
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           )
LOGGING 
NOCOMPRESS 
NOCACHE
MONITORING;


CREATE INDEX MANITOU_TEST.APEX$_WS_HISTORY_IDX ON MANITOU_TEST.APEX$_WS_HISTORY
(WS_APP_ID, DATA_GRID_ID, ROW_ID)
LOGGING
TABLESPACE USERS
PCTFREE    10
INITRANS   2
MAXTRANS   255
STORAGE    (
            INITIAL          64K
            NEXT             1M
            MINEXTENTS       1
            MAXEXTENTS       UNLIMITED
            PCTINCREASE      0
            BUFFER_POOL      DEFAULT
           );
